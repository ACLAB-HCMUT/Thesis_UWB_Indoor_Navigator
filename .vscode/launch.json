	"version": "0.2.0",
	"configurations": [
		{
			"name": "uwb_app",
			"cwd": "uwb_app",
			"request": "launch",
			"type": "dart",
			"args": [
				"d",
				"chrome",
				"--web-port",
				"5000",
				"--web-browser-flag",
				"--disable-web-security"
			]
		},
		{
			"name": "uwb_app (profile mode)",
			"cwd": "uwb_app",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile"
		},
		{
			"name": "uwb_app (release mode)",
			"cwd": "uwb_app",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release"
		}
	]
}
